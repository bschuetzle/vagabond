<div>
  Welcome, <%= @user.name %>!
</div>

<%= @user.current_location %>
<%= @user.email %>
<%= link_to image_tag(@user.photo.url(:profile), class: 'media-object'), @user.photo.url, target: '_blank' %>

<%= button_to "Delete", @user, method: :delete %>
<%= button_to "Edit", edit_user_path, method: :get %>
