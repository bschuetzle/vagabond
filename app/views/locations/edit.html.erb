<h1>Post a New Location</h1>

<%= form_for @location do |f| %>
  City: <%= f.text_field :city %> <br>
  State: <%= f.text_field :state %> <br>
  Region: <%= f.text_field :region %> <br>
  Latitude: <%= f.text_field :lat %> <br>
  Longitude: <%= f.text_field :long %> <br>
  Image: <%= f.file_field :photo %> <br>
  <%= f.submit "Save Changes" %>
<% end %>
